{
	"schemaVersion": 1,
	"id": "riftup",
	"version": "${version}",
	"name": "Riftup",
	"description": "A small progression mod, designed for the needs of Rift Realms Season 3",
	"authors": [
		"Farpo",
		"DinoTheOogle"
	],
	"contact": {
		"homepage": "https://nexusrealms.de/",
		"sources": "https://github.com/Nexusrealms/riftup"
	},
	"license": "MIT",
	"icon": "assets/riftup/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"de.nexusrealms.riftup.Riftup"
		],
		"client": [
			"de.nexusrealms.riftup.client.RiftupClient"
		],
		"fabric-datagen": [
          "de.nexusrealms.riftup.datagen.RiftupDataGenerator"
		],
		"emi": [
			"de.nexusrealms.riftup.compat.emi.RiftupEmiPlugin"
		]
	},
	"mixins": [
		"riftup.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.16.10",
		"minecraft": "~1.21.1",
		"java": ">=21",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}